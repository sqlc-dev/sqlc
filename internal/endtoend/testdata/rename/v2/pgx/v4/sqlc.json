{
  "version": "2",
  "sql": [
    {
      "schema": "schema.sql",
      "queries": "query.sql",
      "engine": "postgresql",
      "gen": {
        "go": {
          "sql_package": "pgx/v4",
          "package": "querytest",
          "out": "go",
          "rename": {
            "id_old": "IDNew",
            "bar_old": "BarNew",
            "foo_old": "FooNew",
            "ip_protocol": "IPProtocol",
            "ip_protocol_tcp": "IPProtocolTCP"
          }
        }
      }
    }
  ]
}
