{
  "version": "1",
  "packages": [
    {
      "path": "go",
      "name": "override",
      "engine": "sqlite",
      "schema": "schema.sql",
      "queries": "query.sql",
      "overrides": [
        {
          "go_struct_tag": "a:\"b\" x:\"y,z\"",
          "column": "foo.tagged"
        },
        {
          "go_struct_tag": "also:\"tagged\"",
          "column": "*.also_tagged"
        },
        {
          "go_struct_tag": "tag_with_space:\" it's legal!\"",
          "column": "*.tag3"
        },
        {
          "db_type": "text",
          "go_struct_tag": "utype:\"notnull_text\""
        },
        {
          "db_type": "text",
          "go_type": "string",
          "nullable": true,
          "go_struct_tag": "utype:\"nullable_text\""
        }
      ]
    }
  ]
}
