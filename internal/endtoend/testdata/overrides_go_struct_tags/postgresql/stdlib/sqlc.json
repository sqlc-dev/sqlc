{
  "version": "1",
  "packages": [
    {
      "path": "go",
      "name": "override",
      "engine": "postgresql",
      "schema": "schema.sql",
      "queries": "query.sql",
      "overrides": [
        {
          "column": "foo.id",
          "go_struct_tag": "x:\"y\""
        },
        {
          "db_type": "text",
          "go_struct_tag": "utype:\"notnull_text\""
        },
        {
          "db_type": "text",
          "go_type": "string",
          "nullable": true,
          "go_struct_tag": "utype:\"nullable_text\""
        }
      ]
    }
  ]
}
