{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "schema": "schema.sql",
      "queries": "query.sql",
      "gen": {
        "go": {
          "package": "querytest",
          "sql_package": "pgx/v5",
          "out": "go",
          "inflection_exclude_table_names": [
            "my_data",
            "exclusions"
          ]
        }
      }
    }
  ]
}
