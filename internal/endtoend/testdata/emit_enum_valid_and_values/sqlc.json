{
  "version": "1",
  "packages": [
    {
      "path": "go",
      "engine": "postgresql",
      "sql_package": "pgx/v4",
      "name": "querytest",
      "schema": "schema.sql",
      "queries": "query.sql",
      "emit_enum_valid_method": true,
      "emit_all_enum_values": true
    }
  ],
  "rename": {
    "id_old": "IDNew",
    "bar_old": "BarNew",
    "foo_old": "FooNew",
    "ip_protocol": "IPProtocol",
    "ip_protocol_tcp": "IPProtocolTCP"
  }
}
